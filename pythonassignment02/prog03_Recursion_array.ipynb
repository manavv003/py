   "source": [
    "#03 Print a list recursively. \n",
    "\n",
    "def print_list_recursion(l, index=0):\n",
    "\n",
    "    if index == len(l):\n",
    "        return 1\n",
    "    \n",
    "    print(l[index])\n",
    "    \n",
    "    print_list_recursion(l, index + 1)\n",
    "\n",
    "my_list = [1, 2, 3, 4, 5, 6, 7, 8, 9]\n",
    "print_list_recursion(my_list)\n"
   ]
