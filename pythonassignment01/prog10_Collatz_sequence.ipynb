    "#10 Collatz sequence\n",
    "n = int(input(\"Input a positive integer (n): \"))\n",
    "\n",
    "print(n, end=\" \")\n",
    "\n",
    "while n != 1:\n",
    "    if n % 2 == 0:\n",
    "        n = int(n / 2)\n",
    "    else:\n",
    "        n = 3 * n + 1\n",
    "    print(n, end=\" \")"
   ]
